<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="MultipleStatusView">
        <attr name="loadingView" format="reference" />
        <attr name="errorView" format="reference" />
        <attr name="emptyView" format="reference" />
        <attr name="noNetworkView" format="reference" />
        <attr name="contentView" format="reference" />
    </declare-styleable>

    <declare-styleable name="ActionBarView">
        <attr name="mode_right">
            <enum name="type_default" value="1000" />
            <enum name="type_two" value="1001" />
            <enum name="type_three" value="1002" />
        </attr>
        <attr name="mode_title" format="string" />
        <attr name="mode_right_title" format="string" />
    </declare-styleable>
</resources>